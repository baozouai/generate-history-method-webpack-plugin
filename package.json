{
  "name": "genetate-history-method-webpack-plugin",
  "version": "0.2.1",
  "packageManager": "pnpm@8.3.1",
  "description": "A webpack Plugin for automatically generating routing methods based on conventional routing",
  "author": "baozouai <baozouai@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/baozouai/genetate-history-method-webpack-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/baozouai/genetate-history-method-webpack-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/baozouai/genetate-history-method-webpack-plugin/issues"
  },
  "keywords": [
    "webpack",
    "plugin",
    "enhance react history",
    "webpack-plugin",
    "add react history method"
  ],
  "main": "dist/genetate-history-method-webpack-plugin.js",
  "types": "dist/genetate-history-method-webpack-plugin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint . --fix",
    "build": "tsc",
    "test": "vitest",
    "tag": "bumpp",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "glob": "^10.2.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.36.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.15.13",
    "bumpp": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "pnpm": "^8.3.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "webpack": "^5.87.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": "eslint --fix"
  }
}
